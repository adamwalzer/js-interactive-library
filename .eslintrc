{
    "parser": "babel-eslint",
    "plugins": [
        "react",
        "babel"
    ],
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "modules": true,
            "arrowFunctions": true,
            "blockBindings": true,
            "classes": true,
            "defaultParams": true,
            "destructuring": true,
            "forOf": true,
            "objectLiteralComputedProperties": true,
            "objectLiteralShorthandMethods": true,
            "objectLiteralShorthandProperties": true,
            "restParams": true,
            "spread": true,
            "superInFunctions": true,
            "templateStrings": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules": {
        "react/jsx-uses-vars": 2,
        "comma-dangle": 0,
        "indent": [
            2,
            2
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            0,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "valid-jsdoc": 0,
        "curly": [2, "multi-line"],
        "dot-notation": 2,
        "eqeqeq": [2, "allow-null"],
        "no-alert": 2,
        "no-caller": 2,
        "no-labels": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-implied-eval": 2,
        "no-loop-func": 2,
        "no-multi-spaces": 2,
        "no-native-reassign": 2,
        "no-new-wrappers": 2,
        "no-with": 2,
        "radix": 2,
        "vars-on-top": 0,
        "wrap-iife": 2,
        "no-shadow": 2,
        "array-bracket-spacing": [2, "never"],
        "block-spacing": [2, "always"],
        "brace-style": 2,
        "camelcase": 2,
        "comma-spacing": [2, {"before": false, "after": true}],
        "eol-last": 2,
        "jsx-quotes": [2, "prefer-double"],
        "key-spacing": 2,
        "new-cap": [2, {"newIsCap": true, "capIsNew": false}],
        "no-trailing-spaces": 2,
        "space-before-function-paren": [2, {"anonymous": "always", "named": "never"}],
        "keyword-spacing": [2, {"after": true, "before": true, "overrides": {"catch": {"after": false}}}],
        "space-infix-ops": 2,
        "space-unary-ops": [1, { "words": true, "nonwords": false }],
        "wrap-regex": 2,
        "arrow-spacing": 2,
        "no-const-assign": 2,
        "no-dupe-class-members": 2,
        "no-this-before-super": 2
    },
    "env": {
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended"
}
